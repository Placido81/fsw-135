-vote-part2/client/src/App.js",[],["54","55"],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/context/UserProvider.js",["56"],"import React, { useState } from 'react';\nimport axios from 'axios';\n\nexport const UserContext = React.createContext()\n\nconst userAxios = axios.create()\n\n\nexport default function UserProvider(props){\n  const initState = { \n    user: JSON.parse(localStorage.getItem(\"user\")) || {}, \n    token: localStorage.getItem(\"token\") || \"\", \n    issues: [],\n    errMsg: ''\n  }\n\n  const [userState, setUserState] = useState(initState)\n\n  function handleAuthErr(errMsg){\n    setUserState(prevState => ({\n      ...prevState,\n      errMsg\n    }))\n  }\n\n\n\n  function signup(credentials){\n    axios.post(\"/auth/signup\", credentials)\n      .then(res => {\n        const { user, token } = res.data\n        localStorage.setItem(\"token\", token)\n        localStorage.setItem(\"user\", JSON.stringify(user))\n        setUserState(prevUserState => ({\n          ...prevUserState,\n          user,\n          token\n        }))\n      })\n      .catch(err => handleAuthErr(err.response.data.errMsg))\n  }\n\n  function login(credentials){\n    axios.post(\"/auth/login\", credentials)\n      .then(res => {\n        const { user, token } = res.data\n        \n        setUserState(prevUserState => ({\n          ...prevUserState,\n          user,\n          token\n        }))\n      })\n      .catch(err => handleAuthErr(err.response.data.errMsg))\n  }\n\n  function logout(){\n    localStorage.removeItem(\"token\")\n    localStorage.removeItem(\"user\")\n    setUserState({\n      user: {},\n      token: \"\",\n      issues: []\n      \n    })\n  }\n\n  \n  function getUserIssues(){\n    userAxios.get(\"/api/issue/user\")\n      .then(res => {\n        setUserState(prevState => ({\n          ...prevState,\n          issues: res.data\n        }))\n      })\n      .catch(err => console.log(err.response.data.errMsg))\n  }\n\n  function addIssue(newIssue){\n    userAxios.post(\"/api/issue\", newIssue)\n      .then(res => {\n        setUserState(prevState => ({\n          ...prevState,\n          issues: [...prevState.issues, res.data]\n        }))\n      })\n      .catch(err => console.log(err.response.data.errMsg))\n  }\n\n  return (\n    <UserContext.Provider\n      value={{\n        ...userState,\n        signup,\n        login,\n        logout,\n        addIssue,\n         }}>\n      { props.children }\n    </UserContext.Provider>\n  )\n}\n","/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/Profile.js",["57","58"],"import React from 'react'\nimport IssueForm from './IssueForm.js'\nimport IssueList from './IssueList.js'\nimport Issue from './Issue.js'\n\n\n\nexport default function Profile(){\n    return (\n      <div className=\"profile\">\n        <h1>Welcome @Username!</h1>\n        <h3>Add A Issue</h3>\n        <IssueForm />\n        <h3>Your Issues</h3>\n      </div>\n    )\n  }\n  ","/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/Auth.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/Public.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/Navbar.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/AuthForm.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/ProtectedRoute.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/Issue.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/IssueForm.js",[],"/Users/markolivo/Documents/bryanuniversity/fsw-135/week4/rock-the-vote-part2/client/src/components/IssueList.js",["59"],"import React from 're